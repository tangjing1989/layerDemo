<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-consumer"/>
    <dubbo:registry check="false" address="zookeeper://localhost:2181"/>

    <dubbo:reference interface="com.tangjing.dubbo.DubboDemoService" id="testService"/>

    <dubbo:service interface="com.tangjing.dubbo.DubboDemoService" ref="testService1"/>
    <bean id="testService1" class="com.tangjing.dubbo.impl.DubboDemoServiceImpl"></bean>
</beans>